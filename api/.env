# File Upload
UPLOAD_DIR=./uploads
MAX_FILE_SIZE=200000000

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Processing
MAX_CONCURRENT_JOBS=5
JOB_TIMEOUT_MS=1800000
