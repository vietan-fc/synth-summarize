PORT=3001
NODE_ENV=development
CORS_ORIGINS=http://localhost:5173,http://localhost:8081,http://localhost:8080

# Firebase (leave empty for mock mode)
FIREBASE_PROJECT_ID=
FIREBASE_PRIVATE_KEY=
FIREBASE_CLIENT_EMAIL=

# OpenAI (leave empty for mock mode)
